{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "port": 9229
    },
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/app/app.js"
    }
  ]
}
